        <div class="layui-input-block">
                <input type="text" name="collectDate" id="collectDate" <#if (examinesB.collectDate)??>value="${examinesB.collectDate?string('yyyy-MM-dd')}"</#if>  lay-verify="date" placeholder="请选择收集时间" autocomplete="off" class="layui-input">

