    </div>
</form>
<script type="text/javascript" src="${base}/static/layui/layui.js"></script>
<script>
    layui.use(['form','jquery','layer','laydate'],function(){
        var form      = layui.form,
                $     = layui.jquery,
                layer = layui.layer;

                          //初始赋值

                          laydate.render({
                            elem: '#collectDate'
<#if (examinesB.collectDate)??>
                            ,value: '${examinesB.collectDate?string("yyyy-MM-dd")}'
</#if>
                          });


        form.on("submit(addExaminesB)",function(data){
                               if(null === data.field.collectDate || "" ===data.field.collectDate){
                                delete data.field["collectDate"];
                            }else{
                                data.field.collectDate = new Date(data.field.collectDate);
                            }
            var loadIndex = layer.load(2, {
                shade: [0.3, '#333']
            });
            //给角色赋值
            $.post("${base}/examinesB/edit",data.field,function(res){
                layer.close(loadIndex);
                if(res.success){
                    parent.layer.msg("初检耐压编辑成功！",{time:1000},function(){
                        parent.layer.close(parent.editIndex);
                        //刷新父页面
                        parent.location.reload();
                    });
                }else{
                    layer.msg(res.message);
                }
            });
            return false;
        });

    });
</script>
